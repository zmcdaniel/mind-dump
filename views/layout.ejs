<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Mind Dump</title>

    <% include ../partials/css_styling.ejs %>
  </head>
  <body>
    <div class="container">
      <p><strong><%- JSON.stringify(currentUser) %></strong></p>


      <% if(alerts.error) { %>
        <% alerts.error.forEach(function(msg) { %>
          <div class="alert alert-danger" id='alertmsg'><%= msg %></div>
        <% }); %>
      <% } %>

      <% if(alerts.success) { %>
        <% alerts.success.forEach(function(msg) { %>
          <div class="alert alert-success" id='alertmsg'><%= msg %></div>
        <% }); %>
      <% } %>



      <h1>Mind Dump</h1>


      <%- body %>





    </div>

    <script src="//code.jquery.com/jquery.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
  </body>
</html>
